<script>
    $.get("coach/findMoneyByUid", function (data) {
        $("#cmoney").html(data);
    });
    $.get("coach/CoachOrder", function (data) {
        var content = "";
        for(var i = 0 ;i < data.length ; i++){
            content += "<tr>"+
                    "<td>"+data[i].conum+"</td>"+
                "<td>"+data[i].coprice+"</td>"+
                "<td>"+data[i].costate+"</td>"+
                "</tr>";
        }
        $("#CoachOrder").html(content);
    });
</script>
