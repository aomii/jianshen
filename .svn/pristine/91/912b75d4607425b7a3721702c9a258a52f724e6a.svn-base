package com.rabbit.fitness.gym.vo;

public class RecruitVo {
    private String grgender;//性别
    private String minGrprice;//最少的钱
    private String maxGrprice;//最多的钱
    private String gname;//场地

    public String getGrgender() {
        return grgender;
    }

    public void setGrgender(String grgender) {
        this.grgender = grgender;
    }

    public String getMinGrprice() {
        return minGrprice;
    }

    public void setMinGrprice(String minGrprice) {
        this.minGrprice = minGrprice;
    }

    public String getMaxGrprice() {
        return maxGrprice;
    }

    public void setMaxGrprice(String maxGrprice) {
        this.maxGrprice = maxGrprice;
    }

    public String getGname() {
        return gname;
    }

    public void setGname(String gname) {
        this.gname = gname;
    }
}
